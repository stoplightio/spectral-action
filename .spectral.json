        "functionOptions": {
          "notMatch": "eval\\("
        }
      },
      "description": "Markdown descriptions must not have \"eval(\".",
      "message": "Markdown descriptions must not have \"eval(\"."
    },
    "no-script-tags-in-markdown": {
      "given": [
